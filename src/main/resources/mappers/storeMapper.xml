<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.jsL.spring.ex.database.repository.ScoreRepository">
 <select id="selectScoreList"
  resultType="com.jsL.spring.ex.database.domain.Score">
 	SELECT
 	`id`
 	, `name`
 	, `year`
 	, `semester`
 	, `term`
 	, `subject`
 	, `score`
 	, `createAt`
 	, `updateAt`
 	
 	FROM `score`
 </select>
 
 </mapper>